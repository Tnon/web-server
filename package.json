{
  "name": "anxinweb",
  "version": "1209",
  "private": true,
  "scripts": {
    "serve": "vite --open",
    "build": "vite build",
    "preview": "vite preview --port 9999",
    "prepare": "husky install",
    "lint:eslint": "eslint --cache --max-warnings 0 src/**/*.{vue,ts,tsx} --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{ts,json,tsx,css,less,scss,vue,html}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:commit": "lint-staged"
  },
  "dependencies": {
    "@antv/g6": "^4.7.5",
    "@antv/x6": "2.5.1",
    "@antv/x6-vue-shape": "^2.0.9",
    "@vueuse/core": "^8.7.5",
    "ant-design-vue": "4.0.0",
    "axios": "^0.27.2",
    "blueimp-md5": "^2.18.0",
    "dayjs": "^1.10.7",
    "echarts": "^5.4.3",
    "highlight.js": "^11.6.0",
    "jshashes": "^1.0.8",
    "lodash": "^4.17.20",
    "nprogress": "^1.0.0-1",
    "pinia": "^2.0.9",
    "postcss-pxtorem": "^6.0.0",
    "qs": "^5.2.1",
    "spark-md5": "^3.0.2",
    "swiper": "^11.0.3",
    "vkbeautify": "^0.99.3",
    "vue": "^3.3.4",
    "vue-clipboard3": "^1.0.1",
    "vue-i18n": "^9.1.7",
